{
    "compilerOptions": {
      "target": "es2020",
      "allowJs": true,
      "checkJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "lib": [
        "esnext"
      ],
      "noEmit": true
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
      "**/*.js"
    ],
  }
  